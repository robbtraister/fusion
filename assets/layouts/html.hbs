<!DOCTYPE html>
<html>
  <head>
    <title>{{# if title }}{{ title }}{{ else }}Handlebars Layout{{/ if }}</title>

    <script src="/_assets/jquery/js/jquery-3.2.1.min.js" defer="defer"></script>
    <script src="/_assets/bootstrap/js/bootstrap.min.js" defer="defer"></script>

    <script src="/_assets/react/engine.js" defer="defer"></script>
    <script src="/_template{{ uri }}" onerror="notFound()" defer="defer"></script>
    <script src="/_content{{ uri }}?v=content" onerror="notFound()" defer="defer"></script>

    <link rel="stylesheet" type="text/css" href="/_assets/bootstrap/css/bootstrap.min.css" />

    <!-- <link rel="icon" type="image/png" sizes="96x96" href="/_assets/favicon-96x96.png" /> -->
    <!-- <link rel="icon" type="image/png" sizes="32x32" href="/_assets/favicon-32x32.png" /> /-->
    <!-- <link rel="icon" type="image/png" sizes="16x16" href="/_assets/favicon-16x16.png" /> /-->
    <!-- <link rel="stylesheet" type="text/css" href="/_assets/style.css" /> /-->
  </head>
  <body onload="typeof content !== 'undefined' && render(content)">
    <div id="App">
      {{{ body }}}
    </div>
  </body>
</html>
