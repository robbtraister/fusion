{
  "name": "fusion-engine",
  "main": "src/index.js",
  "scripts": {
    "build:all": "webpack --config ./webpack.config.js",
    "build:all:production": "NODE_ENV=production npm run build:all -- --mode production -p",
    "build:combined": "webpack --config ./webpack/combined.js",
    "build:combined:production": "NODE_ENV=production npm run build:combined -- --mode production -p",
    "build:components": "webpack --config ./webpack/components.js",
    "build:components:production": "NODE_ENV=production npm run build:components -- --mode production -p",
    "build:engine": "webpack --config ./webpack/engine.js",
    "build:engine:production": "NODE_ENV=production npm run build:engine -- --mode production -p",
    "build:output-types": "webpack --config ./webpack/output-types.js",
    "build:output-types:production": "NODE_ENV=production npm run build:output-types -- --mode production -p",
    "deploy": "sls deploy",
    "extract:jge": "sleep 10 && node bin/extract",
    "install:bundle": "if [ -f ./bundle/src/package.json ]; then (cd ./bundle/src && npm install --production --unsafe-perm); fi",
    "generate:manifest": "node ./manifest/generate",
    "prebuild:all": "rm -rf \"./bundle/dist/\"* && rm -rf \"./bundle/generated/\"* && npm run generate:manifest",
    "prebuild:all:production": "npm run install:bundle",
    "prebuild:combined": "npm run generate:manifest",
    "prebuild:components": "npm run generate:manifest",
    "prebuild:engine": "npm run generate:manifest",
    "prebuild:output-types": "npm run generate:manifest",
    "predeploy": "npm install && npm run build:all:production",
    "prestart": "npm run watch:all &",
    "start": "sls --stage=localhost offline start",
    "start:dev": "npm run install:bundle && npm run start",
    "watch:all": "npm run build:all -- --watch",
    "watch:combined": "npm run build:combined -- --watch",
    "watch:components": "npm run build:components -- --watch",
    "watch:engine": "npm run build:engine -- --watch",
    "watch:output-types": "npm run build:output-types -- --watch"
  },
  "dependencies": {
    "@arc-fusion/prop-types": "^0.1.2",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-root-import": "^6.1.0",
    "body-parser": "^1.18.3",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "debug": "^3.2.5",
    "dynamoose": "^0.8.7",
    "express": "^4.16.3",
    "glob": "^7.1.3",
    "graphql": "^0.13.2",
    "handlebars-webpack-plugin": "^1.5.0",
    "html-react-parser": "^0.4.7",
    "ignore-loader": "^0.1.2",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.2",
    "mock-require": "^3.0.2",
    "on-build-webpack": "^0.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sass": "^1.13.4",
    "sass-loader": "^7.1.0",
    "serverless-http": "^1.6.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^2.1.5",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-sources": "^1.2.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.313.0",
    "mongodb": "^3.1.4",
    "serverless-offline": "^3.25.11"
  }
}
